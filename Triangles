public class Triangle {
    private Point v1, v2, v3;

    public Triangle(Point a, Point b, Point c) {
    }

    public Triangle(double x1, double y1, double x2, double y2, double x3, double y3) {
        Point a = new Point(x1, y1);
        Point b = new Point(x2, y2);
        Point c = new Point(x3, y3);
    }

    public double getPerimeter(Triangle given) {
        double perimeter = 0;
        double side1 = given.v1.distanceTo(given.v2);
        double side2 = given.v2.distanceTo(given.v3);
        double side3 = given.v3.distanceTo(given.v1);
        perimeter = side1 + side2 + side3;
        return perimeter;
    }
    public double getArea(Triangle given) {
        double s = given.getPerimeter(given) / 2;
        double side1 = given.v1.distanceTo(given.v2);
        double side2 = given.v2.distanceTo(given.v3);
        double side3 = given.v3.distanceTo(given.v1);
        double area = Math.sqrt((s * (s - side1) * (s - side2) * (s - side3)));
        return area;
    }
    public String classify(Triangle given) {
        double side1 = given.v1.distanceTo(given.v2);
        double side2 = given.v2.distanceTo(given.v3);
        double side3 = given.v3.distanceTo(given.v1);
        if (side1 == side2 && side1 == side3) {
            return "equilateral";
        } else {
            if (side1 == side2 || side1 == side3 || side2 == side3) {
                return "isosceles";
            } else {
                return "scalene";
            }
        }
    }
}
